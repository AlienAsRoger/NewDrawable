<resources>

	<!-- Base application theme. -->
	<style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
		<!-- Customize your theme here. -->
		<item name="android:windowBackground">@drawable/img_theme_felt</item>

	</style>



	<style name="Text" parent="@android:style/TextAppearance.Small">
		<item name="android:textSize">@dimen/default_text_size</item>
		<item name="android:textColor">@color/new_normal_grey</item>
		<item name="android:textStyle">normal</item>
	</style>

	<style name="Text.Button">
		<item name="android:focusable">true</item>
		<item name="android:clickable">true</item>
		<item name="android:textSize">@dimen/default_button_text_size</item>
		<item name="android:gravity">center</item>
		<item name="android:paddingLeft">8dp</item>
		<item name="android:paddingRight">8dp</item>
		<item name="android:paddingTop">8dp</item>
		<item name="android:paddingBottom">8dp</item>
		<!--
				Don't use in general case
				<item name="android:layout_marginTop">2dp</item>
				<item name="android:layout_marginBottom">2dp</item>
		-->
		<item name="android:minWidth">@dimen/button_min_width</item>
		<item name="android:minHeight">@dimen/main_button_height</item>
		<item name="android:textColor">@color/white</item>

		<!-- Add Shadow Top to text -->
		<!--
				<item name="android:shadowColor">@color/back_shadows_chess</item>
				<item name="android:shadowRadius">0.5</item>
				<item name="android:shadowDx">0</item>
				<item name="android:shadowDy">-1.0</item>
		-->

	</style>

	<style name="Button.Upgrade" parent="Button.Blue">
		<item name="android:minWidth">@dimen/wide_button_min_width</item>
		<item name="android:minHeight">@dimen/small_orange_button_height</item>
	</style>

	<style name="GlyphIcon">
		<!--<item name="android:padding">@dimen/glyph_icon_padding</item>-->
		<item name="android:gravity">center</item>
		<!--<item name="android:textSize">@dimen/glyph_icon_default</item>-->
		<item name="ttf">Icon</item>

		<!-- No Shadow -->
		<item name="android:shadowRadius">0</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">0</item>
	</style>

	<style name="GlyphIcon.Small">
		<!--<item name="android:textSize">@dimen/glyph_icon_small</item>-->
	</style>

	<style name="GlyphIcon.Big">
		<!--<item name="android:textSize">@dimen/glyph_icon_big</item>-->
		<item name="themeColor">true</item>
	</style>

	<style name="GlyphIcon.Square">
		<item name="android:padding">3dp</item>
		<!--<item name="android:textSize">@dimen/glyph_icon_big</item>-->
	</style>

	<style name="GlyphIcon.RoundBack">
		<item name="android:padding">0dp</item>
		<item name="android:textColor">@color/new_light_grey_2</item>
		<!--<item name="android:textSize">@dimen/glyph_icon_round_back</item>-->
		<!--<item name="android:background">@drawable/video_play_btn_back_small_selector</item>-->
