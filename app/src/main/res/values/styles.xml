	</style>

	<style name="GlyphIcon.RoundBack.Play">
		<!--<item name="android:textColor">@color/play_orange</item>-->
		<item name="android:background">@null</item>

		<!-- Shadow -->
		<item name="android:shadowColor">@color/back_shadows_chess</item>
		<item name="android:shadowRadius">0.5</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">1.0</item>
	</style>

	<style name="RightIcon" parent="GlyphIcon.Small">
